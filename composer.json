    "name": "openthinklabs/extension-tao-lti",
    "type" : "tao-extension",
    "extra" : {
    	"tao-extension-name" : "taoLti"
    },
    "require": {
        "php": "~8",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "openthinklabs/oatbox-extension-installer": "dev-master",
        "openthinklabs/lib-lti1p3-ags": "dev-main",
        "openthinklabs/lib-lti1p3-core": "dev-master",
        "openthinklabs/generis": "dev-master",
        "openthinklabs/tao-core": "dev-master"
    },
    "autoload" : {
        "psr-4" : {
            "oat\\taoLti\\" : ""
        }
    },
    "autoload-dev" : {
      "psr-4" : {
        "OAT\\Library\\Lti1p3Core\\Tests\\": "../vendor/oat-sa/lib-lti1p3-core/tests/"
      }
    }
}
